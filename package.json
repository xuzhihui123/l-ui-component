{
  "name": "l-ui-component",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -C play website:dev",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "build": "pnpm run -C build build",
    "buildPublish": "cross-env IS_PUBLISH=true pnpm run -C build build",
    "build:theme": "pnpm run -C packages/theme-chalk build",
    "build:util": "pnpm run -C packages/utils build",
    "ccm": "node  build/node-scripts/createComponent.ts",
    "rcm": "node build/node-scripts/removeComponent.ts",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "prettier": "prettier --write .",
    "lint": "eslint  ./ --fix --ext .ts,.tsx,.vue,.js,.jsx"
  },
  "keywords": [],
  "author": "Lancer",
  "license": "ISC",
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.3.0",
